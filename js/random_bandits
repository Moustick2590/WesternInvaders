var image = document.getElementById("");
    var random_images_array = ["wanted_bandit2.png", "wanted_bandit3.png", "wanted_bandit4.png"];
    
    function getRandomImage(){
        var stockNb = Math.floor(Math.random() * Math.floor(3));
        image.innerHTML = "<img src='images/"+ random_images_array[stockNb] +" '>";
    }